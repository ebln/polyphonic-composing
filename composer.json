{
    "name": "ebln/parasite",
    "type": "library",
    "require": {
        "php": "^8.1 || ^8.2",
        "symfony/console": "^6.2",
        "ebln/provoke-composer-conflict-foo": "^0.1.0"
    },
    "autoload": {
        "psr-4": {
            "Ebln\\ParasiteDemo\\": "src/"
        },
        "exclude-from-classmap": ["parasite/"]
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.29"
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "echo Install PARASITE",
            "cd parasite && composer install"
        ]
    }
}
